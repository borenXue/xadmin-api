{
  "name": "xadmin-api",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "TS_NODE_PROJECT=true && concurrently -p \"[{name}]\" -n 'TSC,RUN' -c 'bgCyan.bold,bgYellow.bold' \"npm run start:tsc\" \"npm run start:node\"",
    "start:tsc": "tsc --watch",
    "start:node": "nodemon -r tsconfig-paths/register dist/index.js",
    "build:test": "rm -rf dist && tsc --outDir dist && npm run sync-files",
    "build:online": "rm -rf dist && tsc --outDir dist && npm run sync-files",
    "deploy:test": "npm run build:test && ./script/test/deploy.sh",
    "run:test": "cross-env NODE_ENV=test node dist/index.js",
    "sync-files": "cp -R config dist/config && cp package* dist/",
    "lint": "npm-run-all --parallel lint:*",
    "lint:editorconfig": "eclint check --block_comment_start '/**' --block_comment_end '*/'",
    "lint:eslint": "eslint --ext .js,.ts src"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cache-manager": "^2.10.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/deep-freeze": "^0.1.2",
    "@types/express": "^4.17.2",
    "@types/js-yaml": "^3.12.1",
    "@types/md5": "^2.1.33",
    "@types/redis": "^2.8.14",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "eclint": "^2.8.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "array-to-tree": "^3.3.2",
    "body-parser": "^1.19.0",
    "cache-manager": "^2.10.1",
    "cache-manager-redis-store": "^1.5.0",
    "change-case": "^3.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "deep-freeze": "0.0.1",
    "express": "^4.17.1",
    "js-yaml": "^3.13.1",
    "md5": "^2.2.1",
    "merge-anything": "^2.4.4",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.22",
    "typeorm-typedi-extensions": "^0.2.3"
  }
}
